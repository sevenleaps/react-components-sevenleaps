  },
  "dependencies": {
    "@kadira/storybook": "^1.13.0",
    "aphrodite": "^0.2.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "react-addons-css-transition-group": "^15.0.1"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
