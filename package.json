{
  "name": "react-components-sevenleaps",
  "version": "0.0.0",
  "description": "Reuseable React Components by Sevenleaps",
  "repository": {
    "type": "git",
    "url": "https://github.com/you/repo.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
